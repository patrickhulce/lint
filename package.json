{
  "name": "@patrickhulce/lint",
  "version": "0.0.0",
  "description": "Lints JavaScript.",
  "main": "./lib/lint.js",
  "scripts": {
    "update-node-expectations":
      "rm -fR test/fixtures/node-expected && mv test/fixtures/node-actual test/fixtures/node-expected",
    "update-react-expectations":
      "rm -fR test/fixtures/react-expected && mv test/fixtures/react-actual test/fixtures/react-expected",
    "test": "npm run test:unit && npm run test:lint",
    "test:unit": "mocha --timeout 10000 --reporter spec --require test/bootstrap test/lint.test.js",
    "test:lint": "./bin/lint.js"
  },
  "bin": {
    "lint": "./bin/lint.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickhulce/lint.git"
  },
  "author": "Patrick Hulce <patrick.hulce@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patrickhulce/lint/issues"
  },
  "homepage": "https://github.com/patrickhulce/lint#readme",
  "dependencies": {
    "babel-eslint": "^7.2.1",
    "debug": "^2.6.3",
    "eslint": "^3.19.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-react": "^6.10.3",
    "lodash": "^4.17.4",
    "prettier": "^1.9.2",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "execa": "^0.6.3",
    "glob": "^7.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "ncp": "^2.0.0",
    "tmp": "^0.0.31",
    "webpack": "^2.3.2"
  }
}
